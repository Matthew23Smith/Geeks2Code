<div class="well col-xs-8 col-xs-offset-2">
  <table class="table table-bordered">
    <tr>
      <td align="center">
        <h1>Task</h1>
      </td>
    </tr>
    <tr>
      <td>
        <%= @task.name %>
      </td>
    </tr>
    <tr>
      <td>
        <strong>Complete?:</strong>&nbsp;&nbsp;<%= @task.status %>
      </td>
    </tr>
    <tr>
      <td>
        <%= link_to 'Edit', edit_task_path(@task), class: "btn btn-info" %> |
        <%= link_to 'Back', tasks_path, class: "btn btn-success" %>
      </td>
    </tr>
  </table>
</div>



